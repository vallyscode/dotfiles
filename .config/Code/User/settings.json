{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Cloud",
    "workbench.editor.decorations.badges": true,
    "workbench.editor.decorations.colors": true,
    "editor.fontFamily": "'Cascadia Code SemiLight','mononoki NF','mononoki Nerd Font Mono'",
    "editor.fontSize": 12,
    "editor.fontLigatures": true,
    "editor.lineHeight": 17,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.renderFinalNewline": true,
    "editor.renderIndentGuides": true,
    "editor.codeLens": true,
    "editor.codeLensFontSize": 12,
    "editor.codeLensFontFamily": "'Cascadia Code SemiLight','mononoki NF','mononoki Nerd Font Mono'",
    "editor.minimap.enabled": false,
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.wordWrap": "off",
    "editor.snippetSuggestions": "top",
    "editor.showFoldingControls": "always",
    "editor.foldingHighlight": true,
    "editor.foldingStrategy": "auto",
    "editor.bracketPairColorization.enabled": true,
    "diffEditor.codeLens": true,
    "terminal.integrated.fontFamily": "'Cascadia Code SemiLight','mononoki NF','mononoki Nerd Font Mono'",
    "terminal.integrated.fontSize": 12,
    "vim.smartRelativeLine": true,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.ignorecase": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "a"
            ],
            "commands": [
                "workbench.action.toggleActivityBarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<tab>"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "<S-tab>"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "d",
                "f"
            ],
            "commands": [
                "editor.action.formatSelection"
            ]
        }
    ],
    "window.menuBarVisibility": "toggle",
    "workbench.tree.renderIndentGuides": "always",
    "java.project.importOnFirstTimeStartup": "automatic",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "redhat.telemetry.enabled": false,
    "java.implementationsCodeLens.enabled": true,
    "java.referencesCodeLens.enabled": true,
    "go.toolsManagement.autoUpdate": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "typescript.referencesCodeLens.showOnAllFunctions": true,
    "rust-analyzer.lens.methodReferences": true,
    "rust-analyzer.lens.references": true,
    "xml.codeLens.enabled": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
}
